    edu_year:
        type: YANDEX.NUMBER
        source: $Edu_year
    edu_program:
        type: EduProgram
        source: $Edu_program
    faculty:
        type: Faculty
        source: $Faculty_
    edu_format:
        source: $Edu_format
    edu_level:
        source: $Edu_level
